{
    "manifest_version": 3,
    "name": "Extension con VITE",
    "description": "Una extensi√≥n de ejemplo con Vite y TypeScript",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    
    "permissions":["activeTab","scripting", "tabs", "storage"]
  }
  